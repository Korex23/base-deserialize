generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Solana
model SolanaOhlc {
  timestamp BigInt    @id
  open      Float
  high      Float
  low       Float
  close     Float

  @@map("solana_ohlc")
}

